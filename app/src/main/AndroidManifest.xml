<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.bermet.ex.helper">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_foreground"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".TheoryActivity"
            android:label="THEORY"/>
        <activity android:name=".ExerciseActivity"
            android:label="EXERCISE"/>
        <activity android:name=".QuizActivity"
            android:label="EASY QUIZ"/>
        <activity
            android:name=".TheoActivity"
            android:label="SIMPLEX"
             />
        <activity
            android:name=".Theo2Activity"
            android:label="DUAL" />
        <activity android:name=".StartingScreenActivity"
            android:label="QUIZ"/>
        <activity android:name=".Quiz2Activity"
            android:label="@string/hardquiz"/>
        <activity android:name=".CalculatorActivity"
            android:label="@string/CALCULATOR"/>
        <activity android:name=".linearActivity"
            android:label="@string/linear"/>
        <activity android:name=".GlossaryActivity"
            android:label="@string/glossary"/>
    </application>

</manifest>